def check_divisibility(num1, num2):

    prod = num1 * num2

    sum_nums = num1 + num2

    if prod % sum_nums == 0:

        return "YEAH"

    else:

        return "NAH"



# Test cases

test_cases = [

    (60, 48),

    (4, 8),

    (-10, 0),

    (12, 34),

    (16, 17)

]



for num1, num2 in test_cases:

    result = check_divisibility(num1, num2)

    print(f"Enter the numbers: {num1} {num2}")

    print(f"Output: {result}\n")

      
