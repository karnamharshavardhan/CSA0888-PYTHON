def length_of_last_word(s):

    words = s.strip().split()  # Remove leading/trailing spaces and split the string

    if words:

        return len(words[-1])

    return 0



# Test cases

test_strings = [

    "Hello World",

    " fly me to the moon ",

    "luffy is still joyboy",

    "123",

    " 45&29 8*6^4"

]



for s in test_strings:

    result = length_of_last_word(s)

    print(f"Input: {s}")

    print(f"Output: {result}\n")

